{
  "compilerOptions": {
    "target": "es5",
    "types":["jest"],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
	"noEmit": true
  },
  "include": [
	  "types.d.ts",
	  "*.ts"
, "tests/db.test.ts"	]
}
